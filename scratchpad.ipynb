    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "ep_rec = []\n",
    "for day in range(365):\n",
    "    e_slope = 0.1/40\n",
    "    e_start = 0.1\n",
    "    sc_EPSILON = np.clip(e_start + e_slope*day,0.1,0.99)\n",
    "    ep_rec.append(sc_EPSILON)"
   ]
